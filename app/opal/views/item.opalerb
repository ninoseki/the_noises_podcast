<div class="item row">
  <div class="col-md-2">
    <p class="pub-date"><%= self.pub_date %></p>
  </div>
  <div class="col-md-10 contents">
    <h4 class="title"><%= self.title %></h4>
    <p class="description"><%= self.description %></p>
    <audio controls>
      <source src="<%= self.link %>" type="audio/mpeg">
    </audio>
  </div>
</div>
